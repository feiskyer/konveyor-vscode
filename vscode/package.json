{
  "name": "aks-migrate",
  "displayName": "AKS Migrate Extension for VSCode",
  "description": "AI-assisted migration and modernization tool for Azure Kubernetes Service",
  "author": "AKS Migrate",
  "icon": "resources/aks-migrate-icon-color.png",
  "license": "Apache-2.0",
  "version": "0.1.0",
  "main": "./out/extension.js",
  "publisher": "Azure",
  "repository": {
    "type": "git",
    "url": "https://github.com/azure/aks-migrate"
  },
  "bugs": "https://github.com/azure/aks-migrate/issues",
  "preview": false,
  "categories": [
    "Programming Languages",
    "Machine Learning",
    "Snippets",
    "Linters"
  ],
  "keywords": [
    "Migration",
    "Migrate",
    "modernizing",
    "MTA",
    "Analyze"
  ],
  "engines": {
    "vscode": "^1.93.0",
    "node": ">=22.9.0",
    "npm": "^9.5.0 || >=10.5.2"
  },
  "activationEvents": [
    "onFileSystem:aksMigrateMemFs",
    "onFileSystem:aksMigrateReadOnly"
  ],
  "contributes": {
    "javaExtensions": [
      "../downloaded_assets/jdtls-bundles/bundle.jar"
    ],
    "commands": [
      {
        "command": "aksmigrate.openProfilesPanel",
        "title": "Manage Analysis Profiles",
        "category": "AKS Migrate",
        "icon": "$(settings-gear)"
      },
      {
        "command": "aksmigrate.modelProviderSettingsOpen",
        "title": "Open the GenAI model provider configuration file",
        "category": "AKS Migrate",
        "icon": "$(gear)"
      },
      {
        "command": "aksmigrate.modelProviderSettingsBackupReset",
        "title": "Open the default the GenAI model provider configuration file and backup the current file",
        "category": "AKS Migrate",
        "icon": "$(gear)"
      },
      {
        "command": "aksmigrate.showAnalysisPanel",
        "title": "Open AKS Migrate Analysis View",
        "category": "AKS Migrate",
        "icon": "$(book)"
      },
      {
        "command": "aksmigrate.showResolutionPanel",
        "title": "Open AKS Migrate Resolution View",
        "category": "AKS Migrate",
        "icon": "$(beaker)"
      },
      {
        "command": "aksmigrate.expandAllIssues",
        "title": "Expand All",
        "category": "AKS Migrate",
        "icon": "$(expand-all)"
      },
      {
        "command": "aksmigrate.expandSingleIssue",
        "title": "Expand All",
        "category": "AKS Migrate",
        "icon": "$(expand-all)"
      },
      {
        "command": "aksmigrate.fixGroupOfIncidents",
        "title": "Resolve incidents",
        "category": "AKS Migrate",
        "icon": "$(debug-console)"
      },
      {
        "command": "aksmigrate.fixIncident",
        "title": "Resolve incident",
        "category": "AKS Migrate",
        "icon": "$(debug-console)"
      },
      {
        "command": "aksmigrate.openAnalysisDetails",
        "title": "Open Details",
        "category": "AKS Migrate",
        "icon": "$(book)"
      },
      {
        "command": "aksmigrate.overrideAnalyzerBinaries",
        "title": "Override Analyzer Binary",
        "category": "AKS Migrate",
        "icon": "$(gear)"
      },
      {
        "command": "aksmigrate.overrideKaiRpcServerBinaries",
        "title": "Override Rpc Server Binary",
        "category": "AKS Migrate",
        "icon": "$(gear)"
      },
      {
        "command": "aksmigrate.configureCustomRules",
        "title": "Configure Custom Rules",
        "category": "AKS Migrate",
        "icon": "$(gear)"
      },
      {
        "command": "aksmigrate.configureSourcesTargets",
        "title": "Configure Analysis Sources and Targets",
        "category": "AKS Migrate",
        "icon": "$(gear)"
      },
      {
        "command": "aksmigrate.configureLabelSelector",
        "title": "Configure Analysis LabelSelector",
        "category": "AKS Migrate",
        "icon": "$(gear)"
      },
      {
        "command": "aksmigrate.startServer",
        "title": "Start Server",
        "category": "AKS Migrate",
        "icon": "$(play)"
      },
      {
        "command": "aksmigrate.stopServer",
        "title": "Stop Server",
        "category": "AKS Migrate",
        "icon": "$(stop)"
      },
      {
        "command": "aksmigrate.restartServer",
        "title": "Restart Server",
        "category": "AKS Migrate",
        "icon": "$(restart)"
      },
      {
        "command": "aksmigrate.runAnalysis",
        "title": "Run Analysis",
        "category": "AKS Migrate",
        "icon": "$(play)"
      },
      {
        "command": "aksmigrate.loadStaticResults",
        "title": "Load results from files",
        "category": "AKS Migrate"
      },
      {
        "command": "aksmigrate.loadResultsFromDataFolder",
        "title": "Reload results",
        "category": "AKS Migrate"
      },
      {
        "command": "aksmigrate.cleanRuleSets",
        "title": "Reset analysis results",
        "category": "AKS Migrate"
      },
      {
        "command": "aksmigrate.loadRuleSets",
        "title": "Load analysis results",
        "category": "AKS Migrate"
      },
      {
        "command": "aksmigrate.loadSolution",
        "title": "Load solution",
        "category": "AKS Migrate"
      },
      {
        "command": "aksmigrate.applyAll",
        "title": "Apply All",
        "category": "AKS Migrate",
        "icon": "$(check)"
      },
      {
        "command": "aksmigrate.discardAll",
        "title": "Discard All",
        "category": "AKS Migrate",
        "icon": "$(discard)"
      },
      {
        "command": "aksmigrate.applyFile",
        "title": "Apply",
        "category": "AKS Migrate",
        "icon": "$(check)"
      },
      {
        "command": "aksmigrate.discardFile",
        "title": "Discard",
        "category": "AKS Migrate",
        "icon": "$(discard)"
      },
      {
        "command": "aksmigrate.copyDiff",
        "category": "AKS Migrate",
        "title": "Copy Diff"
      },
      {
        "command": "aksmigrate.copyPath",
        "category": "AKS Migrate",
        "title": "Copy Path"
      },
      {
        "command": "aksmigrate.diffView.next",
        "category": "AKS Migrate",
        "title": "Go to Next File"
      },
      {
        "command": "aksmigrate.diffView.prev",
        "category": "AKS Migrate",
        "title": "Go to Previous File"
      },
      {
        "command": "aksmigrate.diffView.viewFix",
        "category": "AKS Migrate",
        "title": "View suggested fix"
      },
      {
        "command": "aksmigrate.diffView.applyBlockInline",
        "icon": "$(check)",
        "category": "diffEditor",
        "title": "Apply Block"
      },
      {
        "command": "aksmigrate.diffView.applySelectionInline",
        "icon": "$(check)",
        "category": "diffEditor",
        "title": "Apply Selection"
      },
      {
        "command": "aksmigrate.diffView.applyBlock",
        "icon": "$(arrow-left)",
        "category": "diffEditor",
        "title": "Apply Block"
      },
      {
        "command": "aksmigrate.diffView.applySelection",
        "icon": "$(arrow-left)",
        "category": "diffEditor",
        "title": "Apply Selection"
      },
      {
        "command": "aksmigrate.configureGetSolutionParams",
        "title": "Configure getSolution Parameters",
        "category": "AKS Migrate",
        "icon": "$(gear)"
      }
    ],
    "submenus": [
      {
        "id": "aksmigrate.submenu",
        "label": "AKS Migrate Actions"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "aksmigrate",
          "title": "AKS Migrate",
          "icon": "resources/aks-migrate-icon-color.png"
        }
      ]
    },
    "views": {
      "aksmigrate": [
        {
          "id": "aksmigrate.issueView",
          "name": "AKS Migrate Issues"
        },
        {
          "id": "aksmigrate.diffView",
          "name": "AKS Migrate Resolutions"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "aksmigrate.showAnalysisPanel",
          "group": "navigation@1",
          "when": "view == aksmigrate.issueView"
        },
        {
          "command": "aksmigrate.showResolutionPanel",
          "group": "navigation@2",
          "when": "view == aksmigrate.issueView"
        },
        {
          "command": "aksmigrate.expandAllIssues",
          "group": "navigation@3",
          "when": "view == aksmigrate.issueView"
        },
        {
          "command": "aksmigrate.applyAll",
          "group": "navigation@1",
          "when": "view == aksmigrate.diffView"
        },
        {
          "command": "aksmigrate.discardAll",
          "group": "navigation@2",
          "when": "view == aksmigrate.diffView"
        }
      ],
      "view/item/context": [
        {
          "command": "aksmigrate.fixGroupOfIncidents",
          "group": "inline@3",
          "when": "view == aksmigrate.issueView && viewItem == incident-type-item"
        },
        {
          "command": "aksmigrate.fixIncident",
          "group": "inline@1",
          "when": "view == aksmigrate.issueView && viewItem == reference-item"
        },
        {
          "command": "aksmigrate.expandSingleIssue",
          "group": "inline@2",
          "when": "view == aksmigrate.issueView && viewItem == incident-type-item"
        },
        {
          "command": "aksmigrate.openAnalysisDetails",
          "group": "inline@1",
          "when": "view == aksmigrate.issueView && viewItem == incident-type-item"
        },
        {
          "command": "aksmigrate.applyFile",
          "group": "inline",
          "when": "view == aksmigrate.diffView && viewItem == file-item"
        },
        {
          "command": "aksmigrate.discardFile",
          "group": "inline",
          "when": "view == aksmigrate.diffView && viewItem == file-item"
        },
        {
          "command": "aksmigrate.applyFile",
          "group": "1@1",
          "when": "view == aksmigrate.diffView && viewItem == file-item"
        },
        {
          "command": "aksmigrate.discardFile",
          "group": "1@2",
          "when": "view == aksmigrate.diffView && viewItem == file-item"
        },
        {
          "command": "aksmigrate.copyDiff",
          "group": "2@1",
          "when": "view == aksmigrate.diffView && viewItem == file-item"
        },
        {
          "command": "aksmigrate.copyPath",
          "group": "2@2",
          "when": "view == aksmigrate.diffView && viewItem == file-item"
        }
      ],
      "explorer/context": [
        {
          "submenu": "aksmigrate.submenu",
          "group": "navigation@1"
        }
      ],
      "aksmigrate.submenu": [],
      "commandPalette": [
        {
          "command": "aksmigrate.expandSingleIssue",
          "when": "never"
        },
        {
          "command": "aksmigrate.loadRuleSets",
          "when": "never"
        },
        {
          "command": "aksmigrate.loadSolution",
          "when": "never"
        },
        {
          "command": "aksmigrate.applyFile",
          "when": "never"
        },
        {
          "command": "aksmigrate.discardFile",
          "when": "never"
        },
        {
          "command": "aksmigrate.copyDiff",
          "when": "never"
        },
        {
          "command": "aksmigrate.copyPath",
          "when": "never"
        },
        {
          "command": "aksmigrate.diffView.next",
          "when": "never"
        },
        {
          "command": "aksmigrate.diffView.prev",
          "when": "never"
        },
        {
          "command": "aksmigrate.diffView.viewFix",
          "when": "never"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "AKS Migrate",
      "properties": {
        "aksmigrate.logLevel": {
          "type": "string",
          "enum": [
            "TRACE",
            "DEBUG",
            "INFO",
            "WARN",
            "ERROR",
            "CRITICAL"
          ],
          "default": "DEBUG",
          "description": "Logging level used for the server binaries",
          "scope": "window",
          "order": 0
        },
        "aksmigrate.analyzerPath": {
          "type": "string",
          "default": "",
          "description": "Path to the analyzer binary. If not set, the extension will use the bundled binary.",
          "scope": "resource",
          "order": 2
        },
        "aksmigrate.kaiRpcServerPath": {
          "type": "string",
          "default": "",
          "description": "Path to the rpc-server binary. If not set, the extension will use the bundled binary.",
          "scope": "resource",
          "order": 1
        },
        "aksmigrate.solutionServer.url": {
          "type": "string",
          "default": "http://localhost:8000",
          "description": "URL for the MCP solution server endpoint.",
          "scope": "window",
          "order": 3
        },
        "aksmigrate.solutionServer.enabled": {
          "type": "boolean",
          "default": false,
          "description": "Enable the MCP solution server client.",
          "scope": "window",
          "order": 4
        },
        "aksmigrate.analysis.analyzeOnSave": {
          "type": "boolean",
          "default": true,
          "description": "Should analysis of file be run when it is saved?",
          "scope": "window",
          "order": 6
        },
        "aksmigrate.diffEditorType": {
          "type": "string",
          "enum": [
            "diff",
            "merge"
          ],
          "default": "merge",
          "description": "Select the editor to use when resolving proposed solutions.",
          "scope": "window",
          "order": 7
        },
        "aksmigrate.kai.agentMode": {
          "type": "boolean",
          "default": false,
          "description": "(Experimental) Use agentic flow when getting solution.",
          "scope": "window",
          "order": 6
        },
        "aksmigrate.kai.superAgentMode": {
          "type": "boolean",
          "default": false,
          "description": "(Experimental) Use agentic flow when getting solution.",
          "scope": "window",
          "order": 6
        },
        "aksmigrate.kai.excludedDiagnosticSources": {
          "type": "array",
          "default": [
            "trunk"
          ],
          "description": "List of diagnostic sources to exclude from agentic workflow.",
          "scope": "window",
          "order": 8
        },
        "aksmigrate.kai.getSolutionMaxPriority": {
          "type": [
            "number",
            "null"
          ],
          "default": 0,
          "description": "(Advanced) The maximum priority for issues to be considered by kai in a getSolution request.",
          "scope": "window",
          "order": 40
        },
        "aksmigrate.kai.getSolutionMaxEffort": {
          "type": "string",
          "enum": [
            "Low",
            "Medium",
            "High",
            "Maximum (experimental)"
          ],
          "default": "Low",
          "description": "Defines the effort level for solution generation. Higher values increase processing time.",
          "scope": "window",
          "order": 40
        },
        "aksmigrate.kai.getSolutionMaxLLMQueries": {
          "type": [
            "number",
            "null"
          ],
          "default": null,
          "minimum": 1,
          "description": "(Advanced) Maximum number of LLM queries to be made per solution request (default: No limit)",
          "scope": "window",
          "order": 40
        },
        "aksmigrate.kai.cacheDir": {
          "type": [
            "string",
            "null"
          ],
          "default": null,
          "description": "Path to a directory containing cached responses",
          "scope": "window",
          "order": 50
        },
        "aksmigrate.kai.traceEnabled": {
          "type": "boolean",
          "default": false,
          "description": "Whether to create traces of the communication with the model",
          "scope": "window",
          "order": 50
        },
        "aksmigrate.kai.demoMode": {
          "type": "boolean",
          "default": false,
          "description": "Whether to run Kai in demo mode i.e. use cached LLM responses",
          "scope": "window",
          "order": 50
        },
        "aksmigrate.analysis.incidentLimit": {
          "type": "number",
          "default": 10000,
          "description": "Maximum number of incidents to report",
          "scope": "window",
          "order": 99
        },
        "aksmigrate.analysis.contextLines": {
          "type": "number",
          "default": 10,
          "description": "Number of lines of context to include in incident reports.",
          "scope": "window",
          "order": 99
        },
        "aksmigrate.analysis.codeSnipLimit": {
          "type": "number",
          "default": 10,
          "description": "Number of lines of code to include in incident reports.",
          "scope": "window",
          "order": 99
        },
        "aksmigrate.analysis.analyzeKnownLibraries": {
          "type": "boolean",
          "default": false,
          "description": "Should the analyzer analyze known open-source libraries?",
          "scope": "window",
          "order": 99
        },
        "aksmigrate.analysis.analyzeDependencies": {
          "type": "boolean",
          "default": true,
          "description": "Should the analyzer analyze project dependencies?",
          "scope": "window",
          "order": 99
        },
        "aksmigrate.logging.traceMessageConnection": {
          "type": "boolean",
          "default": false,
          "description": "Trace the jsonrpc calls between the extension and the kai server?",
          "order": 100
        }
      }
    },
    "keybindings": [
      {
        "command": "aksmigrate.diffView.next",
        "key": "f4"
      },
      {
        "command": "aksmigrate.diffView.prev",
        "key": "shift+f4"
      }
    ]
  },
  "includedAssetPaths": {
    "kai": "../downloaded_assets/kai",
    "fernFlowerPath": "../assets/fernflower/fernflower.jar",
    "jdtls": "../downloaded_assets/jdt.ls-1.38.0",
    "jdtlsBundles": "../downloaded_assets/jdtls-bundles",
    "openSourceLabelsFile": "../downloaded_assets/opensource-labels-file/maven.default.index",
    "rulesets": "../downloaded_assets/rulesets"
  },
  "scripts": {
    "clean": "rimraf out",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prebuild": "npm run clean",
    "build": "webpack --mode production",
    "dev": "webpack --watch --mode development",
    "pretest": "npm run build",
    "test": "npm run test:unit-tests && npm run test:integration",
    "test:unit-tests": "mocha",
    "test:integration": "vscode-test"
  },
  "lint-staged": {
    "*.{js,cjs,mjs,ts,cts,mts}": "eslint --fix",
    "*.{css,json,md,yaml,yml}": "prettier --write"
  },
  "devDependencies": {
    "@types/diff": "^6.0.0",
    "@types/jsesc": "^3.0.3",
    "@types/uuid": "^10.0.0",
    "@types/vscode": "^1.93.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.0",
    "@types/mustache": "^4.2.5",
    "diff": "^7.0.0",
    "jsesc": "^3.1.0",
    "mustache": "^4.2.0"
  }
}
